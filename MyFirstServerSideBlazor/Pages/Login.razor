@using MyFirstServerSideBlazor.Componenets.Forms
@inject NavigationManager navigation

<PageTitle>Login</PageTitle>

@{
    if (ShowLogin)
    {
        <LoginForm/>
    }
    else
    {
        <SignUpComponent/>
    }
}

@{
    public bool ShowLogin { get; set; } = true;

    public void MethodCheck()
    {
        navigation
    }
}
